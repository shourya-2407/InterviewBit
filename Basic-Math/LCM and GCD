LCM = n1*n2/ GCD


    long long gcdAB(long long A, long long B){
        if(A == B)
            return A;
        if(A > B)
            gcdAB(A - B, B);
        else
            gcdAB(A, B - A);
    }
  
    vector<long long> lcmAndGcd(long long A , long long B) {
        // code here
        long long gcd = gcdAB(A, B);
        long long lcm = A*B/gcd;
        
        vector<long long> ans;
        ans.push_back(lcm);
        ans.push_back(gcd);
        
        return ans;
    }
