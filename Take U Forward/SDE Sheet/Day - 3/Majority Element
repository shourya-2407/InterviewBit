Java:
class Solution {
    public int majorityElement(int[] nums) {
        int max_ele = 0;
        int cnt = 0;

        for(int i = 0; i < nums.length; i++){
            if(cnt == 0){
                max_ele = nums[i];
            }
            if(nums[i] == max_ele){
                cnt++;
            } else{
                cnt--;
            }
        }

        return max_ele;
    }
}
